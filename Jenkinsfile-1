pipeline {
    agent any
    stages {
        stage('Example Username/Password') {
            environment {
                MY_FILES = sh(script: "cat uat.json | grep image: | awk -F'/' '{print $3}'", returnStdout: true)
            }
            steps {
                sh 'echo "Service user is $MY_FILES"'
            }
        }
    }
}
